      "formik": "^2.4.5",
      "js-cookie": "^3.0.5",
      "jwt-decode": "^4.0.0",
      "lodash": "^4.17.21",
      "react": "^18.2.0",
      "react-chartjs-2": "^5.3.0",
      "react-csv": "^2.2.2",
